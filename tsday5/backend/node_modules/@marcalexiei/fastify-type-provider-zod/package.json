{
  "name": "@marcalexiei/fastify-type-provider-zod",
  "version": "1.2.1",
  "description": "Zod Type Provider for Fastify@5",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "fastify",
    "zod",
    "type",
    "provider"
  ],
  "repository": {
    "url": "https://github.com/marcalexiei/fastify-type-provider-zod"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcalexiei/fastify-type-provider-zod/issues"
  },
  "homepage": "https://marcalexiei.github.io/fastify-type-provider-zod/",
  "author": "turkerdev",
  "maintainers": [
    "marcalexiei"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/cjs/index.d.cts",
      "default": "./dist/cjs/index.cjs"
    },
    "default": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@fastify/error": "^4.2.0"
  },
  "peerDependencies": {
    "@fastify/swagger": "^9.5.1",
    "fastify": "^5.0.0",
    "openapi-types": "^12.1.3",
    "zod": "^3.25.67 || ^4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.6",
    "@fastify/swagger": "9.5.1",
    "@fastify/swagger-ui": "5.2.3",
    "@marcalexiei/biome-config": "1.3.0",
    "@seriousme/openapi-schema-validator": "2.5.0",
    "@types/node": "24.3.0",
    "@vitest/coverage-v8": "3.2.4",
    "fastify": "5.5.0",
    "fastify-plugin": "5.0.1",
    "openapi-types": "12.1.3",
    "typescript": "5.9.2",
    "unplugin-isolated-decl": "0.15.1",
    "vite": "7.1.3",
    "vitepress": "2.0.0-alpha.11",
    "vitest": "3.2.4",
    "zod": "4.0.17"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest --coverage",
    "test:runtime": "vitest --project runtime --coverage",
    "test:types": "vitest --project typecheck",
    "lint": "biome check . && tsc --noEmit",
    "lint:fix": "biome check --write .",
    "release": "pnpm run build && changeset publish",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:lint": "tsc -p examples/tsconfig.json",
    "check-all": "pnpm run build && pnpm run test && pnpm run lint && pnpm run docs:lint"
  }
}